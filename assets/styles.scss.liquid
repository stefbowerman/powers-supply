$header-icon-size: 24px;
$menu-icon-size: 30px;
.site-control {
  pointer-events: none;
  padding: 0;
  margin: 0;
  left: 0;
  right: 0;
  top: 0;
  padding-top: 25px;
  padding-bottom: 25px;
  z-index: 25;
  text-align: center;
  @include vendor-prefix(transition, padding-top 400ms, background 400ms);
  
  a {
    pointer-events: auto;
    display: inline-block;
    color: $header-icon-col;
    vertical-align: middle;
  }
  
  svg {
    vertical-align: top;
    fill: $header-icon-col;
    width: $header-icon-size;
    height: $header-icon-size;
    @include vendor-prefix(transition, fill 400ms);
  }
  
  /* Icon mode */
  &.icons {
    a .text-link { display: none; }
  }
  
  /* Text mode */
  &.text {
    a svg { display: none; }
  }
  
  .menu,
  .cart {
    position: absolute;
    top: 50%;
  }
  
  .cart {
    right: $edge-gutter;
    margin-top: -$header-icon-size/2;
    div {
      position: absolute;
      top: -6px;
      right: -8px;
      width: 2em;
      line-height: 2em;
      text-align: center;
      background: $button-bg-col;
      color: $button-text-col;
      font-family: $body-font-family;
      font-weight: $body-font-weight;
      font-size: 9px;
      border-radius: 100px;
    }
  }
  
  .menu {
    left: $edge-gutter;
    margin-top: -15px;
    svg {
      width: $menu-icon-size;
      height: $menu-icon-size;
    }
    .menu-path-1 {
      transition: transform 500ms ease 100ms, background 400ms ease 0ms;
      transform-origin: left center;
    }
    .menu-path-2 {
      transition: opacity 500ms ease, background 400ms ease;
    }
    .menu-path-3 {
      transition: transform 500ms ease 250ms, background 400ms ease 0ms;
      transform-origin: left center;
    }
  }
  @media (max-width: 767px) {
    .menu {
      left: $mobile-edge-gutter;
    }
    .cart {
      right: $mobile-edge-gutter;
    }
  }
  
  .modal-active & .menu {
    .menu-path-1 {
      transform: rotate(-45deg) translate3d(1px, 3px, 0);
    }
    .menu-path-2 { opacity: 0; }
    .menu-path-3 {
      transform: rotate(45deg) translate3d(1px, -3px, 0);
    }
  }
  
  // for tint on specific pages
  &:before {
    content: '';
    background: transparent;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    opacity: 0;
    transition: opacity 400ms;
  }
}

