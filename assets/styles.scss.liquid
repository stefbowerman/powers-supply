  @media (max-width: 767px) {
    font-size: 1em + ($large-heading-font-size - 1em)/2;
    line-height: $heading-line-height;
  }
}

.overlay {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
}

.overlay .inner {
  display: table;
  width: 100%;
  height: 100%;
}

.overlay .inner > * {
  line-height: 1px;
  display: table-cell;
  vertical-align: middle;
}

.overlay.position-lower-left {
  padding-bottom: $edge-gutter;
  .inner > * {
    vertical-align: bottom;
  }
  .line-1, .line-2, .line-3 {
    text-align: left;
    margin-right: 150px;
  }
}

.theme-modal {
  position: fixed;
  overflow-x: auto;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;/*iOS fix*/
  left: -150%;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 20;
  background: {{ settings.col_mod_bg }};
  background: rgba({{ settings.col_mod_bg }}, {% if settings.use_mod_bg_op %}0.9{% else %}1{% endif %});
  color: {{ settings.col_nav_prim }};
  cursor: pointer;
  
  svg {
    fill: {{ settings.col_nav_prim }};
  }
  
  > .inner {
    min-height: 100%;
    padding: 70px 0 50px;
    
    &, .growth-area {
      display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;

      -webkit-box-direction: normal;
      -webkit-box-orient: vertical;
      -webkit-flex-direction: column;
      -moz-flex-direction: column;
      -ms-flex-direction: column;
      flex-direction: column;
    }
    
    .growth-area {
      -webkit-box-flex: 1;
      -webkit-flex-grow: 1;
      -moz-flex-grow: 1;
      -ms-flex-positive: 1;
      flex-grow: 1;
      
      -webkit-box-pack: center;
      -ms-flex-pack: center;
      -webkit-justify-content: center;
      -moz-justify-content: center;
      justify-content: center;
    }
  }
    
  h1,h2,h3,h4,h5,h6 {
    color: {{ settings.col_nav_prim }};
  }
  
  .container {
    cursor: initial;
  }
  
  &.reveal {
    left: 0;
    -webkit-animation: reveal 300ms ease-out;
    animation: reveal 300ms ease-out;
  }
  
  &.unreveal {
    -webkit-animation: unreveal 250ms ease-out;
    animation: unreveal 250ms ease-out;
  }
  
  &.top-padded > .inner {
    padding-top: 200px;
  }
  &.page {
  }
}

/*only one scrollable container allowed at a time*/
body.modal-active {
  overflow: hidden;
}

#search-modal {
