<div class="central">
  <div class="content {% unless settings.coll_content_bg_show %}transparent{% endunless %}">
    {% if page.content != blank %}
    <h1 class="feature-header large-heading">{{ page.title }}</h1>
    <div class="rte">
      {{ page.content }}
    </div>
    {% else %}
    <h1 class="no-margin">{{ page.title }}</h1>
    {% endif %}
  </div>
</div>

