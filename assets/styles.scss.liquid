/* CSS3 animations */
@-webkit-keyframes reveal {
  0% { opacity:0; }
  1% { opacity:0; -webkit-transform:scale(1.05); }
  100% { opacity: 1; -webkit-transform:scale(1); }
}

@keyframes reveal {
  0% { opacity:0; }
  1% { opacity:0; transform:scale(1.05); }
  100% { opacity: 1; transform:scale(1); }
}

@-webkit-keyframes unreveal {
  0% { opacity:1; left: 0; }
  1% { opacity:1; -webkit-transform:scale(1); }
  100% { opacity: 0; -webkit-transform:scale(1.05); left: 0; }
}

@keyframes unreveal {
  0% { opacity:1; left: 0; }
  1% { opacity:1; transform:scale(1); }
  100% { opacity: 0; transform:scale(1.05); left: 0; }
}


