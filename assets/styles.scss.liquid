/* Yeah boi */
html, body{ min-height:100%; height: 100%; }

/* Responsive images and videos. */
img, iframe { max-width: 100%; vertical-align: top; }
img { height: auto; }

/* Control the pages maximum width. */
.container {
  margin: 0 auto;
  max-width: $site-max-width;
  width: 90%;
}

/* Grids of products and collections, menus, buttons */
.buttons { 
  margin: $gutter 0;
  padding: 0;
}
.grid li, .buttons li, .nav li { 
  list-style-type: none;
  display: inline;
  margin: 0 1em;
  padding: 0;
}
.grid li { 
  padding: 100px;
}
.nav ul,
ul.nav {
  padding: 0;
}

/* Layout */

.side-gutter {
  padding-left: $gutter;
  padding-right: $gutter;
}
.double-gutter.side-gutter {
  padding-left: $gutter * 2;
  padding-right: $gutter * 2;
}
.row {
  margin-left: -$gutter;
  margin-right: -$gutter;
}
.column {
  float: left;
  padding-left: $gutter;
  padding-right: $gutter;
  .double-gutter & {
    padding-left: $gutter * 2;
    padding-right: $gutter * 2;
  }
}
.column.half {
  width: 50%;
}
.column.quarter {
  width: 25%;
}
.column.three-quarters {
  width: 75%;
}
.column.third {
  width: 33.3%;
}
.column.full {
  max-width: 100%;
}

.column.bleed {
  padding-left: 0;
  padding-right: 0;
}

.grid { 
  @extend .row; 
}
.grid li, header, footer, #main {
  @extend .column;
}

header, footer, #main {
  width: 100%;
}

@media screen and (max-width: 767px) {
  body > .container {
    width: auto;
  }
  .column {
    padding-left: 10px;
    padding-right: 10px;
  }
  .column.half,
  .column.third,
  .column.three-quarters {
    float: none;
    width: auto;
  }
  .column.quarter {
    width: 50%;
  }
  .double-gutter .column {
    padding-left: $gutter;
    padding-right: $gutter;
  }
  .side-gutter,
  .double-gutter.side-gutter {
    padding-left: 0;
    padding-right: 0;
  }
}
@media screen and (max-width: 480px) {
  .column.quarter {
    float: none;
    width: auto;
  }
}

/* Number spinners */
input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
}
  
/* Override to globally-hosted search.css on the /search page */
#searchresults {
  margin: 0;
}

#searchresults input {
  width: auto;
}

/* Plugin style overrides */

.cbox-borderless #cboxTopLeft,
.cbox-borderless #cboxTopCenter,
.cbox-borderless #cboxTopRight,
.cbox-borderless #cboxMiddleLeft,
.cbox-borderless #cboxMiddleRight,
.cbox-borderless #cboxBottomLeft,
.cbox-borderless #cboxBottomCenter,
.cbox-borderless #cboxBottomRight {
  visibility: hidden;
}

.cbox-borderless #cboxLoadedContent {
  margin: 0;
}

.cbox-borderless #cboxClose {
  display: none;
}

/* Slick slider */

.slick-slider {
  position: relative;
  display: block;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -ms-touch-action: pan-y;
  touch-action: pan-y;
  -webkit-tap-highlight-color: transparent;
}
.slick-list {
  position: relative;
  overflow: hidden;
  display: block;
  margin: 0;
  padding: 0;

  &:focus {
    outline: none;
  }

  &.dragging {
    cursor: pointer;
    cursor: hand;
  }
}
.slick-slider .slick-track,
.slick-slider .slick-list {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.slick-track {
  position: relative;
  left: 0;
  top: 0;
  display: block;

  &:before,
  &:after {
    content: "";
    display: table;
  }

  &:after {
    clear: both;
  }

  .slick-loading & {
    visibility: hidden;
  }
}
/* FOUC */
.slideshow .slide { display: none; }
.slideshow .slide:first-child { display: block; }
/* /FOUC */
.slick-slide {
  float: left;
  height: 100%;
  min-height: 1px;
  position: relative; // for theme overlays
  
  // background images
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  
  [dir="rtl"] & {
    float: right;
  }
  img {
    display: block;
  }
  &.slick-loading img {
    display: none;
  }

  display: none;

  &.dragging img {
    pointer-events: none;
  }

  .slick-initialized & {
    display: block;
  }

  .slick-loading & {
    visibility: hidden;
  }

  .slick-vertical & {
    display: block;
    height: auto;
    border: 1px solid transparent;
  }
  
  &:focus, a:focus { outline: none; }
}
.slick-arrow.slick-hidden {
  display: none;
}


// Default Variables
$slick-arrow-color: white !default;
$slick-dot-color: $header-icon-col-alt !default;
$slick-opacity-default: 0.75 !default;
$slick-opacity-on-hover: 1 !default;
$slick-opacity-not-active: 0.25 !default;
$slick-prevnext-font-size: 80px;

/* Slider */

.slick-list {
  .slick-loading & {
    background: #fff url({{ 'loading.gif' | asset_url }}) center center no-repeat;
  }
}

/* Arrows */

.slick-prev,
.slick-next {
  position: absolute;
  z-index: 1;
  display: block;
  height: $slick-prevnext-font-size;
  width: $slick-prevnext-font-size;
  line-height: 0px;
  font-size: 0px;
  cursor: pointer;
  background: transparent;
  color: transparent;
  top: 50%;
  -webkit-transform: translate(0, -50%);
  -ms-transform: translate(0, -50%);
  transform: translate(0, -50%);
  padding: 0;
  border: none;
  outline: none;
  min-width: 0;
  &:hover, &:focus {
    outline: none;
    background: transparent;
    color: transparent;
    svg {
      opacity: $slick-opacity-on-hover;
    }
  }
  &.slick-disabled:before {
    opacity: $slick-opacity-not-active;
  }
}

.slick-prev svg,
.slick-next svg {
  width: $slick-prevnext-font-size;
  height: $slick-prevnext-font-size;
  fill: $slick-arrow-color;
  opacity: $slick-opacity-default;
  -webkit-filter: drop-shadow(0px 1px 1px rgba(0,0,0,0.5));
  -moz-filter: drop-shadow(0px 1px 1px rgba(0,0,0,0.5));
  -o-filter: drop-shadow(0px 1px 1px rgba(0,0,0,0.5));
  -ms-filter: drop-shadow(0px 1px 1px rgba(0,0,0,0.5));
  filter: drop-shadow(0px 1px 1px rgba(0,0,0,0.5));
}

.slick-prev {
  left: 0;
  [dir="rtl"] & {
    left: auto;
    right: 0;
  }
}

.slick-next {
  right: 0;
  [dir="rtl"] & {
    left: 0;
    right: auto;
  }
}

/* Dots */

.slick-dots {
  position: absolute;
  bottom: $edge-gutter;
  right: $edge-gutter;
  list-style: none;
  text-align: right;
  margin: 0;
  padding: 0;
  line-height: 1em;
  li {
    position: relative;
    display: inline-block;
    vertical-align: top;
    margin: 0 0 0 15px;
    padding: 0;
    cursor: pointer;
    button {
      border: 2px solid $slick-dot-color;
      background: transparent;
      display: block;
      height: 12px;
      width: 12px;
      min-width: 0;
      outline: none;
      line-height: 0px;
      font-size: 0px;
      color: transparent;
      padding: 0;
      cursor: pointer;
      border-radius: 10px;
      &:hover, &:focus {
        outline: none;
      }
    }
    &.slick-active button {
      background: $slick-dot-color;
    }
  }
}

/* lazy load */
.slick-slide img {
  opacity: 1;
  transition: opacity 250ms;
}
.slick-slide img.slick-loading {
  opacity: 0;
}

.carousel {
  .slick-prev, .slick-next {
    background: $background-col;
    width: 28px;
    height: 28px;
    border-radius: 100px;
    &.slick-disabled, &.theme-unnecessary {
      opacity: 0;
    }
    svg {
      fill: $normal-text-col;
      width: 20px;
      height: 20px;
      -webkit-filter: none;
      -moz-filter: none;
      -o-filter: none;
      -ms-filter: none;
      filter: none;
    }
  }
  .slick-prev {
    left: -14px;
  }
  .slick-next {
    right: -14px;
  }
}

