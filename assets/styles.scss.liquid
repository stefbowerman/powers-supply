  &.caption-hide {
    .caption {
      display: none;
    }
    .image-inner {
      img {
        transition: opacity 0.2s ease-in-out;
        &:nth-child(2) {
          opacity: 0;
          position: absolute;
          top: 0;
          left: 0;
        }
      }
      &:hover {
        img {
          &:nth-child(2) {
/*             opacity: 1; */
          }
        }
      }
    }
  }

  &.caption-over-hover {
    .title, .price {
      position: absolute;
      z-index: 5;
      left: 10px;
      right: 10px;
      opacity: 0;
      @include vendor-prefix(transition, opacity 200ms, margin 200ms ease-in-out);
